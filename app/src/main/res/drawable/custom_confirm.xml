<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="2.3dp"
                android:color="#1A90B9"></stroke>
            <solid
                android:color="#B3D1E9"></solid>
        </shape>
    </item>

    <item>
        <shape>
            <stroke
                android:width="2.3dp"
                android:color="#16AAE0"></stroke>
            <solid
                android:color="#FFF"></solid>
        </shape>
    </item>

</selector>